{
	"name": "ts-parcel-boilerplate",
	"version": "1.0.0",
	"description": "Starter pack for building TypeScript apps",
	"main": "index.js",
	"scripts": {
		"start": "npm run clean && parcel public/index.html --out-dir development -p 8080",
		"build": "parcel build public/index.html --no-cache --out-dir dist --public-url ./",
		"clean": "rimraf ./development && rimraf -rf ./.cache",
		"lint": "./node_modules/.bin/eslint ./src/index.js"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/witoldmetel/TS-Parcel-Boilerplate.git"
	},
	"keywords": [
		"parcel",
		"typescript",
		"react",
		"sass"
	],
	"author": "Witold Metel",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/witoldmetel/TS-Parcel-Boilerplate/issues"
	},
	"homepage": "https://github.com/witoldmetel/TS-Parcel-Boilerplate#readme",
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/runtime-corejs2": "^7.9.2",
		"@typescript-eslint/eslint-plugin": "^2.29.0",
		"@typescript-eslint/parser": "^2.29.0",
		"eslint": "6.8.0",
		"eslint-config-airbnb": "18.1.0",
		"eslint-config-node": "^4.0.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-import": "2.20.1",
		"eslint-plugin-jsx-a11y": "6.2.3",
		"eslint-plugin-node": "^11.1.0",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-react": "7.19.0",
		"eslint-plugin-react-hooks": "2.5.0",
		"parcel-bundler": "^1.12.4",
		"prettier": "^2.0.5",
		"sass": "^1.26.3"
	},
	"dependencies": {
		"@types/jest": "^25.2.1",
		"@types/node": "^13.13.2",
		"@types/react": "^16.9.34",
		"@types/react-dom": "^16.9.6",
		"react": "^16.13.1",
		"react-dom": "^16.13.1",
		"typescript": "^3.8.3"
	}
}
