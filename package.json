{
	"name": "ts-parcel-boilerplate",
	"version": "1.0.0",
	"description": "Starter pack for building TypeScript apps",
	"main": "index.js",
	"scripts": {
		"start": "npm run clean && parcel public/index.html --out-dir development -p 8080",
		"build": "parcel build public/index.html --out-dir dist --public-url ./",
		"clean": "rimraf ./development && rimraf -rf ./.cache"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/witoldmetel/TS-Parcel-Boilerplate.git"
	},
	"keywords": [
		"parcel",
		"typescript",
		"sass"
	],
	"author": "Witold Metel",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/witoldmetel/TS-Parcel-Boilerplate/issues"
	},
	"homepage": "https://github.com/witoldmetel/TS-Parcel-Boilerplate#readme",
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/plugin-transform-runtime": "^7.9.0",
		"@babel/runtime-corejs2": "^7.9.2",
		"parcel-bundler": "^1.12.4",
		"sass": "^1.26.3",
		"uikit": "^3.4.2"
	}
}
